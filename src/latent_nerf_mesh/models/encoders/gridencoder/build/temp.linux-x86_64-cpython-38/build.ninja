ninja_required_version = 1.3
cxx = c++
nvcc = /usr/local/cuda-11.4/bin/nvcc

cflags = -pthread -B /opt/conda/envs/lnerf/compiler_compat -Wno-unused-result -Wsign-compare -DNDEBUG -fwrapv -O2 -Wall -fPIC -O2 -isystem /opt/conda/envs/lnerf/include -fPIC -O2 -isystem /opt/conda/envs/lnerf/include -fPIC -I/opt/conda/envs/lnerf/lib/python3.8/site-packages/torch/include -I/opt/conda/envs/lnerf/lib/python3.8/site-packages/torch/include/torch/csrc/api/include -I/opt/conda/envs/lnerf/lib/python3.8/site-packages/torch/include/TH -I/opt/conda/envs/lnerf/lib/python3.8/site-packages/torch/include/THC -I/usr/local/cuda-11.4/include -I/opt/conda/envs/lnerf/include/python3.8 -c
post_cflags = -O3 -std=c++14 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=_gridencoder -D_GLIBCXX_USE_CXX11_ABI=0
cuda_cflags = -I/opt/conda/envs/lnerf/lib/python3.8/site-packages/torch/include -I/opt/conda/envs/lnerf/lib/python3.8/site-packages/torch/include/torch/csrc/api/include -I/opt/conda/envs/lnerf/lib/python3.8/site-packages/torch/include/TH -I/opt/conda/envs/lnerf/lib/python3.8/site-packages/torch/include/THC -I/usr/local/cuda-11.4/include -I/opt/conda/envs/lnerf/include/python3.8 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -O3 -std=c++14 -U__CUDA_NO_HALF_OPERATORS__ -U__CUDA_NO_HALF_CONVERSIONS__ -U__CUDA_NO_HALF2_OPERATORS__ -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=_gridencoder -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_61,code=compute_61 -gencode=arch=compute_61,code=sm_61
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags



build /source/sihun/stable-dreamfusion/gridencoder/build/temp.linux-x86_64-cpython-38/source/sihun/stable-dreamfusion/gridencoder/src/bindings.o: compile /source/sihun/stable-dreamfusion/gridencoder/src/bindings.cpp
build /source/sihun/stable-dreamfusion/gridencoder/build/temp.linux-x86_64-cpython-38/source/sihun/stable-dreamfusion/gridencoder/src/gridencoder.o: cuda_compile /source/sihun/stable-dreamfusion/gridencoder/src/gridencoder.cu





